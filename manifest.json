{
    "manifest_version": 3,
    "name": "Image Overlay",
    "version": "1.0",
    "description": "silly guys that pop up every time you open a tab!!",
    "permissions": ["storage"],
    "host_permissions": [
        "<all_urls>"
    ],
    "chrome_url_overrides": {
        "newtab": "content.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "gifs/*.gif"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}