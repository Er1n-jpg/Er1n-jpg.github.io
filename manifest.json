{
    "manifest_version": 3,
    "name": "silly",
    "version": "1.0",
    "description": "silly guys that pop up every time you open a tab!!", 
    "action": {
        "default_popup": "popup.html"
    }, 
    "permissions": [
        "storage"
    ],
    "icons": { "16": "icon16.png",
            "32": "icon32.png",
           "48": "icon48.png",
          "128": "icon128.png" }, 
    "content-scripts": [
        {
            "matches": ["<all-urls>"],
            "html": ["content.html"],
            "css": ["content.css"],
            "js": ["content.js"]
        } 
    ]
  }